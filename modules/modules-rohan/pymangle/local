#%Module########################################################################
# vim: set filetype=tcl :

module-whatis	"mangle in python"
conflict pymangle

eval set  [ array get env HOME ]

set PYMANGLE_DIR $HOME/exports/pymangle-local

prepend-path PYTHONPATH $PYMANGLE_DIR/lib/python2.7/site-packages/

setenv PYMANGLE_DIR $PYMANGLE_DIR
