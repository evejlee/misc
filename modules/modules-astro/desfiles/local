#%Module########################################################################
# vim: set filetype=tcl :
##
## espy modulefile
##
proc ModulesHelp { } {
    global version

    puts stderr "	DES configurations, file lists, etc."
    puts stderr "
	Version $version
"
}

# for Tcl script use only
set     version      local

module-whatis	"DES configurations, file lists, etc."

conflict desfiles

setenv DESFILES_DIR /astro/u/astrodat/products-special/desfiles
