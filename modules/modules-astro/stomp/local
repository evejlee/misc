#%Module########################################################################
# vim: set filetype=tcl :

module-whatis	"stomp install straight from svn repo"
conflict stomp

eval set  [ array get env HOME ]

set STOMP_DIR $HOME/exports/stomp-local

prepend-path PYTHONPATH $STOMP_DIR/lib/python2.7/site-packages/

setenv STOMP_DIR $STOMP_DIR
