#%Module########################################################################
# vim: set filetype=tcl :

module-whatis	"numpydb install straight from git repo"
conflict numpydb

eval set  [ array get env HOME ]

set NUMPYDB_DIR $HOME/exports/numpydb-local

prepend-path PYTHONPATH $NUMPYDB_DIR/lib/python2.7/site-packages/

setenv NUMPYDB_DIR $NUMPYDB_DIR

